<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.zhaogaofei.webviewtest.activity.JavaWithJSActivity">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:gravity="center"
        android:text="JAVA 调用 JS 代码"/>

    <TextView
        android:id="@+id/tv_with_js"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:gravity="center"
        android:text="load url"/>

    <TextView
        android:id="@+id/tv_get_with_js"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:gravity="center"
        android:text="get result before 4.4"/>

    <WebView
        android:id="@+id/web_view_with_js"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</LinearLayout>
